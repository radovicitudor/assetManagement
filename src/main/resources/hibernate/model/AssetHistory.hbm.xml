<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ro.sci.asset.model">

    <class name="AssetHistory" table="ASSET_HISTORY">
        <id name="assetId" column="ASSET_ID">
            <generator class="native"/>
        </id>
        <property name="AssetHistoryAdd" type="timestamp" column="ASSET_HISTORY_ADD"/>
        <property name="assetName" column="ASSET_NAME"/>
        <property name="serviceTag" column="SERVICE_TAG"/>
        <property name="description" column="DESCRIPTION"/>
    </class>

</hibernate-mapping>

